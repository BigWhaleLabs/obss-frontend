{
  "name": "obss-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write src/classnames/tailwind.ts",
    "prestart": "yarn generate-css-types",
    "prebuild": "yarn generate-css-types",
    "update": "yarn build && git add . && git commit -m 'update' && git push"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@big-whale-labs/stores": "^0.1.16",
    "@coinbase/wallet-sdk": "^3.5.2",
    "@vitejs/plugin-react": "^2.0.1",
    "@walletconnect/web3-provider": "^1.8.0",
    "assert-browserify": "^2.0.0",
    "envalid": "^7.3.1",
    "eth-rpc-errors": "^4.0.3",
    "preact": "^10.10.6",
    "react-toastify": "^9.0.8",
    "unfetch": "^4.2.0",
    "valtio": "^1.6.4",
    "web3modal": "^1.9.9"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@preact/preset-vite": "^2.3.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-relative-import-paths": "^1.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-valtio": "^0.6.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "rollup-plugin-polyfill-node": "^0.10.2",
    "rollup-plugin-visualizer": "^5.8.0",
    "tailwindcss": "^3.1.8",
    "tailwindcss-classnames": "^3.0.6",
    "typescript": "^4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-remove-console": "^1.1.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "packageManager": "yarn@3.2.1"
}
